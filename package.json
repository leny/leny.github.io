  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leny/leny.github.io"
  },
  "bugs": {
    "url": "https://github.com/leny/leny.github.io"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
