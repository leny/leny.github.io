    "@fortawesome/react-fontawesome": "^0.1.7",
    "@pwops/core": "0.1.4",
    "@pwops/emotion-css": "0.1.4",
    "@pwops/mixins": "0.1.4",
    "color": "^3.1.2",
    "emotion-box-sizing-reset": "^0.2.0",
    "emotion-normalize": "^10.1.0",
    "gatsby": "^2.18.17",
    "gatsby-plugin-emotion": "^4.1.16",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-source-filesystem": "^2.1.39",
    "gatsby-transformer-json": "^2.2.20",
    "luxon": "^1.21.3",
    "markdown-it": "^10.0.0",
